<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.newcapec.citycard.common.po">
	<class
		name="CardPsamLog"
		table="CARD_PSAM_LOG"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
			<generator class="uuid.hex"/>
		</id>

		<property
			name="samcardno"
			column="SAMCARDNO"
			type="integer"
			not-null="true"
			length="22"
		/>
		<property
			name="samcardname"
			column="SAMCARDNAME"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="state"
			column="STATE"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="editPerson"
			column="EDIT_PERSON"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="editDate"
			column="EDIT_DATE"
			type="date"
			not-null="true"
			length="7"
		/>


	</class>	
</hibernate-mapping>