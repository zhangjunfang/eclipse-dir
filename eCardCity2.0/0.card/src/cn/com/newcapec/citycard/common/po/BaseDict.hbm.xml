<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.newcapec.citycard.common.po">
	<class
		name="BaseDict"
		table="BASE_DICT"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
			<generator class="uuid.hex"/>
		</id>

		<property
			name="cCode"
			column="C_CODE"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="dCode"
			column="D_CODE"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="dDesc1"
			column="D_DESC1"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="dDesc2"
			column="D_DESC2"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="dDesc3"
			column="D_DESC3"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="dDesc4"
			column="D_DESC4"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="dDesc5"
			column="D_DESC5"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="status"
			column="STATUS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="allowEdit"
			column="ALLOW_EDIT"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="sortNum"
			column="SORT_NUM"
			type="integer"
			not-null="false"
			length="6"
		/>
		<property
			name="notes"
			column="NOTES"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="version"
			column="VERSION"
			type="integer"
			not-null="false"
			length="5"
		/>


	</class>	
</hibernate-mapping>