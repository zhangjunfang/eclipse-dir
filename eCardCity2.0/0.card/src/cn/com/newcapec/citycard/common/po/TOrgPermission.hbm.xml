<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.newcapec.citycard.common.po">
	<class
		name="TOrgPermission"
		table="T_ORG_PERMISSION"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="ID"
		>
			<generator class="sequence"><param name="sequence">SEQ_T_ORG_PERMISSION</param></generator>
		</id>

		<property
			name="permName"
			column="PERM_NAME"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="permUrl"
			column="PERM_URL"
			type="string"
			not-null="true"
			length="100"
		/>
		<property
			name="pid"
			column="PID"
			type="integer"
			not-null="true"
			length="38"
		/>
		<property
			name="remark"
			column="REMARK"
			type="string"
			not-null="true"
			length="200"
		/>


	</class>	
</hibernate-mapping>