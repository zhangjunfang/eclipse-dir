<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.newcapec.citycard.common.po">
	<class
		name="ConsumeProcessedException"
		table="CONSUME_PROCESSED_EXCEPTION"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
			<generator class="uuid.hex"/>
		</id>

		<property
			name="cousumeProcessedId"
			column="COUSUME_PROCESSED_ID"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="statdate"
			column="STATDATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="industrycode"
			column="INDUSTRYCODE"
			type="string"
			not-null="true"
			length="8"
		/>
		<property
			name="collectpoint"
			column="COLLECTPOINT"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="badrecid"
			column="BADRECID"
			type="string"
			not-null="false"
			length="1"
		/>


	</class>	
</hibernate-mapping>