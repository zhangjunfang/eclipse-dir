<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.newcapec.citycard.common.po">
	<class
		name="Trader"
		table="TRADER"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
			<generator class="uuid.hex"/>
		</id>

		<property
			name="merchantcode"
			column="MERCHANTCODE"
			type="string"
			not-null="true"
			length="8"
		/>
		<property
			name="merchantname"
			column="MERCHANTNAME"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="merchantP"
			column="MERCHANT_P"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="sortNum"
			column="SORT_NUM"
			type="integer"
			not-null="true"
			length="6"
		/>
		<property
			name="industrycode"
			column="INDUSTRYCODE"
			type="string"
			not-null="true"
			length="8"
		/>
		<property
			name="dpjm"
			column="DPJM"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="accountid"
			column="ACCOUNTID"
			type="string"
			not-null="true"
			length="30"
		/>
		<property
			name="linkman"
			column="LINKMAN"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="phone"
			column="PHONE"
			type="string"
			not-null="false"
			length="13"
		/>
		<property
			name="fax"
			column="FAX"
			type="string"
			not-null="false"
			length="13"
		/>
		<property
			name="merchanttype"
			column="MERCHANTTYPE"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="merchatntstatus"
			column="MERCHATNTSTATUS"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="address"
			column="ADDRESS"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="postcode"
			column="POSTCODE"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="bankid"
			column="BANKID"
			type="integer"
			not-null="true"
			length="6"
		/>
		<property
			name="transaccountid"
			column="TRANSACCOUNTID"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="editPerson"
			column="EDIT_PERSON"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="editDate"
			column="EDIT_DATE"
			type="date"
			not-null="true"
			length="7"
		/>


	</class>	
</hibernate-mapping>