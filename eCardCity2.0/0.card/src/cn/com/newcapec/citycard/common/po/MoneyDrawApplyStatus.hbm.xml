<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.newcapec.citycard.common.po">
	<class
		name="MoneyDrawApplyStatus"
		table="MONEY_DRAW_APPLY_STATUS"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
			<generator class="uuid.hex"/>
		</id>

		<property
			name="planid"
			column="PLANID"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="oldstate"
			column="OLDSTATE"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="newstate"
			column="NEWSTATE"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="oldaffairstatus"
			column="OLDAFFAIRSTATUS"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="newaffairstatus"
			column="NEWAFFAIRSTATUS"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="reason"
			column="REASON"
			type="string"
			not-null="true"
			length="200"
		/>
		<property
			name="reasoncode"
			column="REASONCODE"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="asn"
			column="ASN"
			type="integer"
			not-null="false"
			length="20"
		/>
		<property
			name="cardsn"
			column="CARDSN"
			type="integer"
			not-null="false"
			length="5"
		/>
		<property
			name="customerid"
			column="CUSTOMERID"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="poscode"
			column="POSCODE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="businesstype"
			column="BUSINESSTYPE"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="wallettype"
			column="WALLETTYPE"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="saveopcount"
			column="SAVEOPCOUNT"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="opcount"
			column="OPCOUNT"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="editPerson"
			column="EDIT_PERSON"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="editDate"
			column="EDIT_DATE"
			type="date"
			not-null="true"
			length="7"
		/>


	</class>	
</hibernate-mapping>