<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.newcapec.citycard.common.po">
	<class
		name="TraderChargeauthAcc"
		table="TRADER_CHARGEAUTH_ACC"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
			<generator class="uuid.hex"/>
		</id>

		<property
			name="dptcode"
			column="DPTCODE"
			type="string"
			not-null="true"
			length="9"
		/>
		<property
			name="curoddfare"
			column="CURODDFARE"
			type="big_decimal"
			not-null="true"
			length="20"
		/>
		<property
			name="lastoddfare"
			column="LASTODDFARE"
			type="big_decimal"
			not-null="true"
			length="20"
		/>
		<property
			name="opfare"
			column="OPFARE"
			type="big_decimal"
			not-null="true"
			length="20"
		/>
		<property
			name="allowoverdraftfare"
			column="ALLOWOVERDRAFTFARE"
			type="big_decimal"
			not-null="true"
			length="20"
		/>
		<property
			name="maxlimitdate"
			column="MAXLIMITDATE"
			type="integer"
			not-null="true"
			length="3"
		/>
		<property
			name="isuse"
			column="ISUSE"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="sumfare"
			column="SUMFARE"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="customerunitcode"
			column="CUSTOMERUNITCODE"
			type="string"
			not-null="true"
			length="12"
		/>
		<property
			name="dir"
			column="DIR"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="flag"
			column="FLAG"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="changedptcode"
			column="CHANGEDPTCODE"
			type="string"
			not-null="false"
			length="9"
		/>
		<property
			name="typeflag"
			column="TYPEFLAG"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="maxlimitfare"
			column="MAXLIMITFARE"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="editPerson"
			column="EDIT_PERSON"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="editDate"
			column="EDIT_DATE"
			type="date"
			not-null="true"
			length="7"
		/>


	</class>	
</hibernate-mapping>