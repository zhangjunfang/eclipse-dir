<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.newcapec.citycard.common.po">
	<class
		name="CardAppinfo"
		table="CARD_APPINFO"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
			<generator class="uuid.hex"/>
		</id>

		<property
			name="customerid"
			column="CUSTOMERID"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="asn"
			column="ASN"
			type="integer"
			not-null="true"
			length="20"
		/>
		<property
			name="citycardno"
			column="CITYCARDNO"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="scardsnr"
			column="SCARDSNR"
			type="string"
			not-null="true"
			length="16"
		/>
		<property
			name="cardsn"
			column="CARDSN"
			type="integer"
			not-null="true"
			length="5"
		/>
		<property
			name="cardkind"
			column="CARDKIND"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="cardtype"
			column="CARDTYPE"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="cardstatus"
			column="CARDSTATUS"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="flag"
			column="FLAG"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="cardflag"
			column="CARDFLAG"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="carduse"
			column="CARDUSE"
			type="integer"
			not-null="false"
			length="4"
		/>
		<property
			name="pwd"
			column="PWD"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="lossdt"
			column="LOSSDT"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="searchpwd"
			column="SEARCHPWD"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="ver"
			column="VER"
			type="integer"
			not-null="false"
			length="5"
		/>
		<property
			name="markName"
			column="MARK_NAME"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="annualInspection"
			column="IS_ANNUAL_INSPECTION"
			type="true_false"
			not-null="true"
			length="1"
		/>
		<property
			name="annualInspectionStart"
			column="ANNUAL_INSPECTION_START"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="annualInspectionEnd"
			column="ANNUAL_INSPECTION_END"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="editPerson"
			column="EDIT_PERSON"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="editDate"
			column="EDIT_DATE"
			type="date"
			not-null="true"
			length="7"
		/>


	</class>	
</hibernate-mapping>