<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.newcapec.citycard.common.po">
	<class
		name="AddressCollect"
		table="ADDRESS_COLLECT"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
			<generator class="uuid.hex"/>
		</id>

		<property
			name="collectid"
			column="COLLECTID"
			type="string"
			not-null="true"
			length="12"
		/>
		<property
			name="collectname"
			column="COLLECTNAME"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="collectjp"
			column="COLLECTJP"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="collectstatus"
			column="COLLECTSTATUS"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="collecttype"
			column="COLLECTTYPE"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="address"
			column="ADDRESS"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="editPerson"
			column="EDIT_PERSON"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="editDate"
			column="EDIT_DATE"
			type="date"
			not-null="true"
			length="7"
		/>


	</class>	
</hibernate-mapping>