<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.newcapec.citycard.common.po">
	<class
		name="AddressNetSite"
		table="ADDRESS_NET_SITE"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
			<generator class="uuid.hex"/>
		</id>

		<property
			name="netid"
			column="NETID"
			type="string"
			not-null="true"
			length="12"
		/>
		<property
			name="netname"
			column="NETNAME"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="netjp"
			column="NETJP"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="netstatus"
			column="NETSTATUS"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="nettype"
			column="NETTYPE"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="likeman"
			column="LIKEMAN"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="phone"
			column="PHONE"
			type="string"
			not-null="false"
			length="13"
		/>
		<property
			name="address"
			column="ADDRESS"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="fax"
			column="FAX"
			type="string"
			not-null="false"
			length="13"
		/>
		<property
			name="ver"
			column="VER"
			type="integer"
			not-null="true"
			length="5"
		/>
		<property
			name="netidP"
			column="NETID_P"
			type="string"
			not-null="true"
			length="12"
		/>
		<property
			name="netkind"
			column="NETKIND"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="editPerson"
			column="EDIT_PERSON"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="editDate"
			column="EDIT_DATE"
			type="date"
			not-null="true"
			length="7"
		/>


	</class>	
</hibernate-mapping>