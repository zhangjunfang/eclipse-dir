<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.newcapec.citycard.common.po">
	<class
		name="TermInfo"
		table="TERM_INFO"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
			<generator class="uuid.hex"/>
		</id>

		<property
			name="termname"
			column="TERMNAME"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="merchantcode"
			column="MERCHANTCODE"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="poscode"
			column="POSCODE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="isuse"
			column="ISUSE"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="paramgroupid"
			column="PARAMGROUPID"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="typeid"
			column="TYPEID"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="status"
			column="STATUS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="lastcollectdate"
			column="LASTCOLLECTDATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="psamcardno"
			column="PSAMCARDNO"
			type="integer"
			not-null="true"
			length="22"
		/>
		<property
			name="lastupdatedate"
			column="LASTUPDATEDATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="collectid"
			column="COLLECTID"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="siteid"
			column="SITEID"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="isauth"
			column="ISAUTH"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="workingkey"
			column="WORKINGKEY"
			type="string"
			not-null="true"
			length="16"
		/>
		<property
			name="kekcode"
			column="KEKCODE"
			type="string"
			not-null="true"
			length="16"
		/>
		<property
			name="appid"
			column="APPID"
			type="integer"
			not-null="true"
			length="5"
		/>
		<property
			name="kekencryptkey"
			column="KEKENCRYPTKEY"
			type="string"
			not-null="true"
			length="16"
		/>
		<property
			name="isdelete"
			column="ISDELETE"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="ver"
			column="VER"
			type="integer"
			not-null="true"
			length="5"
		/>


	</class>	
</hibernate-mapping>