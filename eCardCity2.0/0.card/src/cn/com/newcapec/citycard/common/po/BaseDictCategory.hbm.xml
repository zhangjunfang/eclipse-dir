<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.newcapec.citycard.common.po">
	<class
		name="BaseDictCategory"
		table="BASE_DICT_CATEGORY"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
			<generator class="uuid.hex"/>
		</id>

		<property
			name="cCode"
			column="C_CODE"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="cName"
			column="C_NAME"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="cType"
			column="C_TYPE"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="allowEdit"
			column="ALLOW_EDIT"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="allowMemory"
			column="ALLOW_MEMORY"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="sortNum"
			column="SORT_NUM"
			type="integer"
			not-null="false"
			length="6"
		/>
		<property
			name="notes"
			column="NOTES"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="version"
			column="VERSION"
			type="integer"
			not-null="false"
			length="5"
		/>


	</class>	
</hibernate-mapping>