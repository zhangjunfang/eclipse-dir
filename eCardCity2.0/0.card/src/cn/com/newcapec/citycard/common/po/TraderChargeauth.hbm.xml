<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.newcapec.citycard.common.po">
	<class
		name="TraderChargeauth"
		table="TRADER_CHARGEAUTH"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
			<generator class="uuid.hex"/>
		</id>

		<property
			name="authcode"
			column="AUTHCODE"
			type="string"
			not-null="true"
			length="10"
		/>
		<property
			name="businesspoint"
			column="BUSINESSPOINT"
			type="string"
			not-null="true"
			length="12"
		/>
		<property
			name="curoddfare"
			column="CURODDFARE"
			type="big_decimal"
			not-null="true"
			length="20"
		/>
		<property
			name="authtime"
			column="AUTHTIME"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="allowoverdraftfare"
			column="ALLOWOVERDRAFTFARE"
			type="big_decimal"
			not-null="true"
			length="20"
		/>
		<property
			name="maxlimitdate"
			column="MAXLIMITDATE"
			type="integer"
			not-null="true"
			length="4"
		/>
		<property
			name="isuse"
			column="ISUSE"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="workstatus"
			column="WORKSTATUS"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="limitenddate"
			column="LIMITENDDATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="maxlimitfare"
			column="MAXLIMITFARE"
			type="big_decimal"
			not-null="true"
			length="20"
		/>
		<property
			name="sumfare"
			column="SUMFARE"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="allowoverdraftopdt"
			column="ALLOWOVERDRAFTOPDT"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="allowoverdraftsumfare"
			column="ALLOWOVERDRAFTSUMFARE"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="editPerson"
			column="EDIT_PERSON"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="editDate"
			column="EDIT_DATE"
			type="date"
			not-null="true"
			length="7"
		/>


	</class>	
</hibernate-mapping>