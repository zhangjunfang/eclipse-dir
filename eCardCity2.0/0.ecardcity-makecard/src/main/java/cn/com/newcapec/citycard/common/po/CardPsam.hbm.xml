<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.newcapec.citycard.common.po">
	<class
		name="CardPsam"
		table="CARD_PSAM"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
			<generator class="uuid.hex"/>
		</id>

		<property
			name="samcardno"
			column="SAMCARDNO"
			type="integer"
			not-null="true"
			length="22"
		/>
		<property
			name="samcardname"
			column="SAMCARDNAME"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="samcardsn"
			column="SAMCARDSN"
			type="integer"
			not-null="true"
			length="22"
		/>
		<property
			name="samcardsnr"
			column="SAMCARDSNR"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="state"
			column="STATE"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="startdate"
			column="STARTDATE"
			type="date"
			not-null="true"
			length="7"
		/>
		<property
			name="enddate"
			column="ENDDATE"
			type="date"
			not-null="true"
			length="7"
		/>
		<property
			name="lossdate"
			column="LOSSDATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="lossrecno"
			column="LOSSRECNO"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="lastupdate"
			column="LASTUPDATE"
			type="date"
			not-null="true"
			length="7"
		/>
		<property
			name="createdate"
			column="CREATEDATE"
			type="date"
			not-null="true"
			length="7"
		/>
		<property
			name="ver"
			column="VER"
			type="integer"
			not-null="true"
			length="22"
		/>
		<property
			name="addtype"
			column="ADDTYPE"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="samcardtype"
			column="SAMCARDTYPE"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="printcode"
			column="PRINTCODE"
			type="string"
			not-null="true"
			length="20"
		/>


	</class>	
</hibernate-mapping>