<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.newcapec.citycard.common.po">
	<class
		name="BaseAppAccess"
		table="BASE_APP_ACCESS"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
			<generator class="uuid.hex"/>
		</id>

		<property
			name="appid"
			column="APPID"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="appname"
			column="APPNAME"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="ipaddess"
			column="IPADDESS"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="appver"
			column="APPVER"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="saltkey"
			column="SALTKEY"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="isused"
			column="ISUSED"
			type="integer"
			not-null="false"
			length="1"
		/>
		<property
			name="memo"
			column="MEMO"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="ver"
			column="VER"
			type="integer"
			not-null="false"
			length="5"
		/>
		<property
			name="secretkey"
			column="SECRETKEY"
			type="string"
			not-null="false"
			length="16"
		/>


	</class>	
</hibernate-mapping>