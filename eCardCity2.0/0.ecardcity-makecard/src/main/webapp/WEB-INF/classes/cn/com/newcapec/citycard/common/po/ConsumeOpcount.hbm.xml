<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="cn.com.newcapec.citycard.common.po">
	<class
		name="ConsumeOpcount"
		table="CONSUME_OPCOUNT"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
			<generator class="uuid.hex"/>
		</id>

		<property
			name="asn"
			column="ASN"
			type="integer"
			not-null="true"
			length="20"
		/>
		<property
			name="opcount"
			column="OPCOUNT"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="wallettype"
			column="WALLETTYPE"
			type="string"
			not-null="true"
			length="1"
		/>


	</class>	
</hibernate-mapping>